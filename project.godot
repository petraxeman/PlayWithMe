; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PlayWithMe"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://icon.svg"

[autoload]

Globals="*res://singletons/Globals.gd"
Aiapi="*res://singletons/AIAPI.gd"

[gui]

theme/custom="res://assets/themes/NightyDog.tres"

[internationalization]

locale/translation_remaps={}
locale/translations=PackedStringArray("res://assets/langs/lang_ru_en.en.translation", "res://assets/langs/lang_ru_en.ru.translation")

[rendering]

renderer/rendering_method="mobile"
